package com.pwdgame.bean;

import android.graphics.drawable.Drawable;

public class PopListItem {

	public PopListItem(){
		
	}
	public PopListItem(int id, CharSequence text, Drawable background) {
		this.id = id;
		this.text=text;
		this.background=background;
	}
	public void set(int id, CharSequence text, Drawable background) {
		this.id = id;
		this.text=text;
		this.background=background;
	}
	public int id;
	public CharSequence text;
	public Drawable background;
}
